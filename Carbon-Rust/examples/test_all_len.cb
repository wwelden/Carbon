// Comprehensive test for .len property on all supported data types
print("Testing .len on all supported data types:");

// Test String
let str = "hello world";
print("String '" + str + "' length: " + toString(str.len));

// Test Array
let arr = [1, 2, 3, 4, 5];
print("Array length: " + toString(arr.len));

// Test ArrayList
let arrayList = ArrayList[10, 20, 30, 40];
print("ArrayList length: " + toString(arrayList.len));

// Test empty structures
let emptyStr = "";
print("Empty string length: " + toString(emptyStr.len));

let emptyArr = [];
print("Empty array length: " + toString(emptyArr.len));

let emptyArrayList = ArrayList[];
print("Empty arraylist length: " + toString(emptyArrayList.len));

// Test mixed content ArrayList
let mixedList = ArrayList["text", 42, true, null];
print("Mixed arraylist length: " + toString(mixedList.len));

print("All .len tests passed!");
