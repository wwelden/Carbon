// Test all new operators

let x = 10;
print("Initial x:");
print(x);

// Test increment
x++;
print("After x++:");
print(x);

// Test compound addition
x += 5;
print("After x += 5:");
print(x);

// Test decrement
x--;
print("After x--:");
print(x);

// Test compound multiplication
x *= 2;
print("After x *= 2:");
print(x);

// Test compound division
x /= 4;
print("After x /= 4:");
print(x);

// Test compound subtraction
x -= 3;
print("After x -= 3:");
print(x);

// Test compound modulo
let y = 17;
print("Testing modulo: y = 17");
y %= 5;
print("After y %= 5:");
print(y);

print("All operator tests complete!");
