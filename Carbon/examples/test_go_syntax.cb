fn add(int a, int b) int {
    return a + b
}

fn loop_example() {
    int[] arr = [1, 2, 3, 10, 5]

    for i in arr {
        if (i == 10) {
            print("found ten!")
        }
    }

    for i = 0; i < arr.len; i++ {
        if (arr[i] == 10) {
            print("found ten at index")
            print(i)
        }
    }

    int count = 0
    for count < 5 {
        print(count)
        count = count + 1
    }
}
