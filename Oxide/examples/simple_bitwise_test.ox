// Simple bitwise operations test

let a = 12;
let b = 10;

print("a = " + toString(a));
print("b = " + toString(b));

print("a & b = " + toString(a & b));
print("a | b = " + toString(a | b));
print("a ^ b = " + toString(a ^ b));

let c = 5;
print("~c = " + toString(~c));

print("a << 1 = " + toString(a << 1));
print("a >> 1 = " + toString(a >> 1));
