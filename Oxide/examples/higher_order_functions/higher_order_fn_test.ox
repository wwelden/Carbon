// Test higher-order functions with fn syntax
print("=== Higher-Order Functions with fn syntax ===")

let numbers = [1, 2, 3, 4, 5]
print("Original numbers: " + toString(numbers))

// Test map with single parameter arrow function
let doubled = map(x => x * 2)(numbers)
print("map(x => x * 2): " + toString(doubled))

// Test filter with single parameter arrow function
let filtered = filter(x => x > 3)(numbers)
print("filter(x => x > 3): " + toString(filtered))

// Test fold with fn syntax for two parameters
let sum = fold(fn(acc, x) -> acc + x)(0)(numbers)
print("fold(fn(acc, x) -> acc + x)(0): " + toString(sum))

print("=== Higher-order fn test complete ===")
